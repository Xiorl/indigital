{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "linebreak-style": "off",
        "quotes": [
            "error",
            "double"
        ],
        "react/jsx-no-useless-fragment": "warn",
        "react/jsx-filename-extension": "off",
        "indent": ["error", 3]
    }
}